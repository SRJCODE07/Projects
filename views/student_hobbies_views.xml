<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="student_hobbies_views_tree" model="ir.ui.view">
            <field name="name">student.hobbies.views.tree </field>
            <field name="model">student.hobbies</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="student_hobbies_action" model="ir.actions.act_window">
            <field name="name">Student Hobbies </field>
            <field name="res_model">student.hobbies</field>
            <field name="view_id" ref="student_hobbies_views_tree"/>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>