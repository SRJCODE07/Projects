<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="college_department_views_tree" model="ir.ui.view">
            <field name="name">college.department.views.tree </field>
            <field name="model">college.department</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="college_department_action" model="ir.actions.act_window">
            <field name="name">College Department </field>
            <field name="res_model">college.department</field>
            <field name="view_id" ref="college_department_views_tree"/>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>